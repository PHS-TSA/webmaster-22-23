{
  "name": "webmaster-22-23",
  "version": "0.1.0",
  "description": "Home of the world's first cosmic cruise line!",
  "private": true,
  "main": "src/js/main.ts",
  "directories": {
    "doc": "docs",
    "lib": "src"
  },
  "type": "module",
  "scripts": {
    "start": "npx run-pty run-pty.json",
    "test": "npx vitest",
    "test:no-watch": "npx vitest run",
    "coverage": "npx vitest run --coverage",
    "dev": "npx vite --host",
    "build": "npm run check && npx vite build",
    "check": "npx tsc",
    "check:watch": "npx tsc -w",
    "preview": "npx vite preview --host",
    "lint": "npx eslint .",
    "lint:spell": "npx cspell lint --gitignore \"**/*.{txt,js,html,ts,md,json,cff,jsonc}\"",
    "lint:commit": "npx commitlint --config commitlint.config.cts --edit",
    "lint:markdown": "npx markdownlint-cli2",
    "lint:markdown:fix": "npx markdownlint-cli2-fix",
    "lint:markdown:links": "npx markdown-link-check docs/CHANGELOG.md docs/sources.md --config mlc-config.json"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@commitlint/format": "^17.4.0",
    "@types/bootstrap": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vitest/ui": "^0.28.5",
    "conventional-changelog-atom": "^2.0.8",
    "cspell": "^6.24.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "markdown-link-check": "^3.10.3",
    "markdownlint-cli2": "^0.6.0",
    "prettier": "2.8.4",
    "run-pty": "^4.0.3",
    "sass": "^1.58.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-lightningcss": "^0.0.3",
    "vite-plugin-pwa": "^0.14.4",
    "vite-plugin-virtual-mpa": "^1.6.0",
    "vite-plugin-webfont-dl": "^3.6.0",
    "vitest": "^0.28.5"
  },
  "optionalDependencies": {
    "eslint-module-utils": "^2.7.4"
  },
  "engines": {
    "node": ">=16.0"
  },
  "repository": {
    "type": "git",
    "url": "github:PHS-TSA/webmaster-22-23"
  },
  "contributors": [
    {
      "name": "Elisha Dukes",
      "email": "88557639+lishaduck@users.noreply.github.com",
      "url": "https://lishaduck.github.io"
    }
  ],
  "funding": [
    {
      "url": "https://phs-tsa.github.io/donations/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PHS-TSA/webmaster-22-23/issues"
  },
  "homepage": "https://github.com/PHS-TSA/webmaster-22-23#readme"
}
